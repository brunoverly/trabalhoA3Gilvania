<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.trabalhoA3Gilvania.controller.ImportOsController">
   <children>
      <Button fx:id="importSelecionarExcel" layoutX="395.0" layoutY="98.0" mnemonicParsing="false" onAction="#importSelecionarExcelOnAction" text="Selecionar Arquivo">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="importFazerImport" layoutX="447.0" layoutY="340.0" mnemonicParsing="false" onAction="#importFazerImportOnAction" prefHeight="27.0" prefWidth="74.0" style="-fx-background-color: #EBB672;" text="Importar">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <TextField fx:id="importNumeroOs" layoutX="73.0" layoutY="99.0" prefHeight="25.0" prefWidth="312.0" promptText="Ex: 987654200" />
      <Label layoutX="73.0" layoutY="69.0" prefHeight="17.0" prefWidth="231.0" text="Digite o numero da Os a ser importada">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Button fx:id="importVoltar" layoutX="354.0" layoutY="340.0" mnemonicParsing="false" onAction="#importVoltarOnAction" prefHeight="27.0" prefWidth="62.0" text="Voltar">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <AnchorPane prefHeight="40.0" prefWidth="600.0" style="-fx-background-color: #EBB672;">
         <children>
            <Label layoutX="91.0" layoutY="7.0" text="Importar Os">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
